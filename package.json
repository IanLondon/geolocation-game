{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate: React hot reloading with css modules and standard linter",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "standard",
    "build": "npm run lint && webpack -p && cp index.html dist/",
    "deploy:gh-pages": "npm run build && ./deploy-gh-pages.sh",
    "start": "webpack-dev-server --hot --host 0.0.0.0"
  },
  "author": "Ian London",
  "license": "UNLICENSED",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "webrtc-adapter": "^5.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "standard": "^10.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  }
}
